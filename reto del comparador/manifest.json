{
  "manifest_version": 3,
  "name": "Comparador de Precios por Keyword",
  "version": "1.0",
  "description": "Compara precios entre Falabella y MercadoLibre por keyword",
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": [
    "https://www.falabella.com/*",
    "https://listado.mercadolibre.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
